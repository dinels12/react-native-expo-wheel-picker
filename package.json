{
  "name": "react-native-expo-modern-wheel-picker",
  "version": "0.2.1",
  "description": "Modern wheel picker for React Native and Expo",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "!lib/typescript/example",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "test": "jest",
    "build": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepare": "bob build"
  },
  "keywords": [
    "react-native",
    "expo",
    "wheel-picker",
    "picker",
    "ios",
    "android"
  ],
  "repository": "https://github.com/dinels12/react-native-expo-wheel-picker",
  "author": "Daniel <dinels12@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dinels12/react-native-expo-wheel-picker/issues"
  },
  "homepage": "https://github.com/dinels12/react-native-expo-wheel-picker#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@types/react": "^19.0.10",
    "eslint": "^8.57.0",
    "prettier": "^3.2.0",
    "react": "^18.3.1",
    "@types/jest": "^26.0.0",
    "expo-linear-gradient": "~14.1.5",
    "react-native": "^0.76.0",
    "react-native-builder-bob": "^0.30.0",
    "typescript": "^5.3.0",
    "babel-preset-expo": "^13.0.2"
  },
  "peerDependencies": {
    "react": ">=19.0.0",
    "expo": ">=53.0.0",
    "react-native": ">=0.79.0"
  },
  "dependencies": {
    "react-native-reanimated": "^3.17.4"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
